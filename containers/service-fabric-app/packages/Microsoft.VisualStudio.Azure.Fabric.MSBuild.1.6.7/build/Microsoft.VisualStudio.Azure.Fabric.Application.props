<Project ToolsVersion="14.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Import Project="$(MSBuildExtensionsPath)\$(MSBuildToolsVersion)\Microsoft.Common.props" Condition="Exists('$(MSBuildExtensionsPath)\$(MSBuildToolsVersion)\Microsoft.Common.props')" />

  <PropertyGroup>
    <PlatformTarget>x64</PlatformTarget>
    <Configuration Condition=" '$(Configuration)' == '' ">Debug</Configuration>
    <OutputPath Condition=" '$(OutputPath)' == '' ">bin\$(Configuration)\</OutputPath>
    <AddAdditionalExplicitAssemblyReferences>false</AddAdditionalExplicitAssemblyReferences>
    <DebugSymbols>false</DebugSymbols>
    <Platform>x64</Platform>
    <SkipCopyBuildProduct>true</SkipCopyBuildProduct>
    <TargetRuntime>None</TargetRuntime>
    <ApplicationMSBuildNuGetPackageVersion>1.6.7</ApplicationMSBuildNuGetPackageVersion>
    <ApplicationDebugMode Condition=" '$(ApplicationDebugMode)' == '' ">Refresh</ApplicationDebugMode>
    <PackageOutputFolder Condition=" '$(PackageOutputFolder)' == '' ">true</PackageOutputFolder>
    <OutputType>FabricApplication</OutputType>
  </PropertyGroup>
</Project>
